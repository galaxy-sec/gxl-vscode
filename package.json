{
  "name": "gxl",
  "displayName": "GXL",
  "description": "Galaxy Flow Language",
  "version": "0.1.8",
  "icon": "images/gxl.png",
  "publisher": "galaxy-sec",
  "license": "MIT",
  "engines": {
    "vscode": "*"
  },
  "scripts": {
    "update-grammar": "node ./build/update-grammar.mjs"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "gxl",
        "extensions": [
          ".gxl"
        ],
        "aliases": [
          "GXL",
          "Gxl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "gxl",
        "path": "./syntaxes/gxl.tmLanguage.json",
        "scopeName": "source.gxl"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/galaxy-sec/gxl-vscode.git"
  }
}
